{
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"quotes": [
			"error",
			"single"
		],
		"no-console": [
			"off"
		],
		"no-empty": [
			"off"
		],
		"no-unused-vars": [
			"off"
		]
	}
}